{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "windows-server/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "windows-server/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
        "uhfHeaderId": "MSDocsHeader-WindowsIT", 
		"breadcrumb_path": "/windows-server/breadcrumbs/toc.json",
    "extendBreadcrumb":  true,
	    "feedback_system": "GitHub",
        "feedback_github_repo": "MicrosoftDocs/windowsserverdocs",
        "feedback_product_url": "https://windowsserver.uservoice.com/forums/295047-general-feedback",
		"_op_documentIdPathDepotMapping": {
			"./": {
			  "depot_name": "WS.WindowsServerDocs"
			}
		}
	},
    "fileMetadata": {
      "feedback_product_url": {
        "manage/windows-admin-center/**/*.md": "http://aka.ms/wacfeedback" 
      }
    },
    "template": [
      null
    ],
    "dest": "windows-server"
  }
}
