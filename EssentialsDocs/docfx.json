{
   "build": {
      "content": [
         {
            "files": [
               "**/*.md",
               "**/*.yml"               
            ],
            "exclude": [
               "**/obj/**",
               "**/includes/**"
            ]
         }
      ],
      "resource": [
         {
            "files": [
               "**/*.png",
               "**/*.jpg",
               "**/*.gif",
               "**/*.json",
               "**/*.svg"     
            ],
            "exclude": [
               "**/obj/**",
               "**/includes/**"
            ]
         }
      ],
      "overwrite": [],
      "externalReference": [],
      "globalMetadata": {
         "breadcrumb_path": "/windows-server-essentials/breadcrumbs/toc.json",
         "uhfHeaderId": "MSDocsHeader-WindowsServer",
